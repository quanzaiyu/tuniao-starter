    "@iconify-json/mdi": "^1.1.53",
    "@iconify/json": "^2.2.95",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@unocss/extractor-pug": "^0.50.8",
    "@volar-plugins/vetur": "^0.1.0",
    "@vue-macros/reactivity-transform": "^0.2.5",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.7",
    "esbuild": "0.17.19",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-vue-pug": "^0.6.0",
    "jest": "27.0.4",
    "jest-environment-node": "27.5.1",
    "lint-staged": "^13.2.3",
    "miniprogram-api-typings": "^3.10.0",
    "only-allow": "^1.1.1",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "pug": "^3.0.2",
    "sass": "^1.64.1",
    "simple-git-hooks": "^2.9.0",
    "standard-changelog": "^2.0.27",
    "stylelint": "^15.10.2",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylus": "^0.59.0",
    "typescript": "^5.1.6",
    "unocss": "^0.50.8",
    "unocss-applet": "^0.3.8",
    "unplugin-auto-import": "^0.15.3",
    "vite": "4.0.3",
    "vite-plugin-babel": "^1.1.3",
    "vitepress": "1.0.0-alpha.60"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec commitlint --config commitlint.config.js --edit"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --cache --fix"
    ],
    "src/**/*.{css,vue}": [
      "stylelint --fix"
    ]
  },
  "browserslist": [
    "Android >= 4",
    "ios >= 8"
  ],
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}